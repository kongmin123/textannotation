<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.annotation.dao.TaskLabelMapper">

    <insert id="insertTaskLabel" useGeneratedKeys="true" keyProperty="id" parameterType="com.annotation.model.TaskLabel">
        insert into task_label (taskid,labelid) values (#{taskid},#{labelid})
    </insert>

</mapper>